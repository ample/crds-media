    {%- assign messages = site.messages | reverse -%}
    {%- for message in messages -%}
      {% if message.video_file %}
        <item>
          <title>{{ message.title | xml_escape }}</title>
          <itunes:author>Crossroads</itunes:author>
          <description>{{ message.description | xml_escape }}</description>
          <itunes:summary>{{ message.description | xml_escape }}</itunes:summary>
          <itunes:explicit>no</itunes:explicit>
          <itunes:duration>{{ message.video_duration }}</itunes:duration>
          <pubDate>{{ message.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
          <link>https:{{ message.video_file }}</link>
          <enclosure url="https:{{ message.video_file }}" type="video/mp4" length="123456" />
          <guid isPermaLink="false">https:{{ message.video_file }}</guid>
          <feedburner:origLink>https:{{ message.video_file }}</feedburner:origLink>
          <media:content url="https:{{ message.video_file }}" type="video/mp4" />
          <itunes:subtitle>{{ message.description | xml_escape }}</itunes:subtitle>
        </item>
      {% endif %}
    {%- endfor -%}
